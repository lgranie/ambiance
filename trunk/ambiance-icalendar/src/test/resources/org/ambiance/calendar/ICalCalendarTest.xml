<component-set>
  <components>
    <component>
      <role>org.ambiance.calendar.AmbianceCalendar</role>
      <role-hint>ical</role-hint>
      <implementation>
        org.ambiance.calendar.FootTVICalCalendar
      </implementation>
      <description></description>
      <requirements>
        <requirement>
          <role>org.ambiance.transport.Transporter</role>
          <role-hint>default</role-hint>
          <field-name>transporter</field-name>
        </requirement>
        <requirement>
          <role>org.ambiance.calendar.AmbianceCalendarUpdater</role>
          <role-hint>fr-football-tv</role-hint>
          <field-name>updater</field-name>
        </requirement>
      </requirements>
      <configuration>
        <id>fr-footbal-tv</id>
        <timezone>Europe/Paris</timezone>
        <input-url>
          http://laurent.granie.free.fr/calendars/football-tv.ics
        </input-url>
        <output-url>
          ftp://laurent.granie:motdepasse@ftpperso.free.fr/calendars/football-tv.ics
        </output-url>
      </configuration>
    </component>
    <component>
      <role>org.ambiance.calendar.AmbianceCalendarUpdater</role>
      <role-hint>fr-football-tv</role-hint>
      <implementation>
        org.ambiance.calendar.TvFootUpdater
      </implementation>
      <description></description>
      <requirements>
        <requirement>
          <role>org.ambiance.transport.Transporter</role>
          <role-hint>default</role-hint>
          <field-name>transporter</field-name>
        </requirement>
      </requirements>
    </component>
  </components>
</component-set>
